# mount point		fstype	device						flags
/system			ext4	/dev/block/mapper/system						flags=display="System";
#/system_image		emmc	/dev/block/mapper/system						flags=display="System Image";backup=0;flashimg=1;
/system_ext		ext4	/dev/block/mapper/system_ext					flags=display="System_ext";
#/system_ext_image	emmc	/dev/block/mapper/system_ext					flags=display="System_ext Image";backup=0;flashimg=1;
/vendor			ext4	/dev/block/mapper/vendor						flags=display="Vendor";
#/vendor_image		emmc	/dev/block/mapper/vendor						flags=display="Vendor image";backup=0;flashimg=1;
/odm			ext4	/dev/block/mapper/odm						flags=display="Odm";
#/odm_image		emmc	/dev/block/mapper/odm						flags=display="Odm image";backup=0;flashimg=1;
/product		ext4	/dev/block/mapper/product						flags=display="Product";
#/product_image		emmc	/dev/block/mapper/product						flags=display="Product image";backup=0;flashimg=1;
/boot			emmc	/dev/block/bootdevice/by-name/boot		flags=display="Boot";backup=1;flashimg=1;
/recovery		emmc	/dev/block/bootdevice/by-name/recovery		flags=display="Recovery";backup=1;flashimg=1;
/metadata		ext4	/dev/block/bootdevice/by-name/metadata		flags=display="Metadata";
/data			f2fs	/dev/block/bootdevice/by-name/userdata		flags=display="Data";
/cache			ext4	/dev/block/bootdevice/by-name/cache		flags=display="Cache";
/efs			ext4	/dev/block/bootdevice/by-name/sec_efs		flags=display="Efs";
/sec_efs		ext4	/dev/block/bootdevice/by-name/sec_efs		flags=display="Sec_efs";
/carrier		ext4	/dev/block/bootdevice/by-name/carrier		flags=display="Carrier";
/preload		ext4	/dev/block/bootdevice/by-name/hidden		flags=display="Preload";
/modem			emmc	/dev/block/bootdevice/by-name/apnhlos		flags=display="Modem";
/mdm			emmc	/dev/block/bootdevice/by-name/modem		flags=display="Mdm";
/dsp			emmc	/dev/block/bootdevice/by-name/dsp		flags=display="Dsp";
/misc			emmc	/dev/block/bootdevice/by-name/misc		flags=display="Misc";backup=1;flashimg=1;
/dtbo			emmc	/dev/block/bootdevice/by-name/dtbo		flags=display="Dtbo";backup=1;flashimg=1;
/prism_image		emmc	/dev/block/bootdevice/by-name/prism		flags=display="Prism";backup=1;flashimg=1;
/optics_image		emmc	/dev/block/bootdevice/by-name/optics		flags=display="Optics";backup=1;flashimg=1;
/vbmeta_samsung		emmc	/dev/block/bootdevice/by-name/vbmeta_samsung	flags=display="Vbmeta_samsung";
/vbmeta_system		emmc	/dev/block/bootdevice/by-name/vbmeta_system	flags=display="Vbmeta_system";

# Removeable Storage
/external_sd		vfat	/dev/block/mmcblk0p1	/dev/block/mmcblk0	flags=display="Sdcard";storage;wipeingui;removable;
/usb-otg		vfat	/dev/block/sde1		/dev/block/sde		flags=display="USB-OTG";storage;wipeingui;removable;
